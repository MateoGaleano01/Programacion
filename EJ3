#include <iostream>

using namespace std;

int EstMayor(int a, int b, int c) {
    int mayor = a;
    if (b > mayor) {
        mayor = b;
    }
    if (c > mayor) {
        mayor = c;
    }
    return mayor;
}

int main() {
    int num1, num2, num3;
    cout << "Ingrese tres numeros separados por un espacio: ";
    cin >> num1 >> num2 >> num3;
    int mayor = EstMayor(num1, num2, num3);
    cout << "El mayor numero es " << mayor << "\n";
    return 0;
}
